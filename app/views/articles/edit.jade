.page-header
  h1 Edit article

- form_for(article, {action: path_to.article(article), method: 'PUT', id: "article_form"}, function (form) {
  != partial('articles/form.jade', {locals: {form: form, article: article}})
  .actions
    != form.submit('Update article', {class: 'btn btn-primary'})
    span= ' or '
    != link_to('Cancel', path_to.article(article), {class: 'btn'})
- });
