!!! 5
html
  head
    title= title
    meta(http-equiv="content-type", content="text/html; charset=utf-8")
    meta(http-equiv="X-UA-Compatible", content="IE=edge,chrome=1")
    meta(name="viewport", content="width=device-width, initial-scale=1, maximum-scale=1")
    block css
      != stylesheet_link_tag("http://fonts.googleapis.com/css?family=Open+Sans:400,600,300,800,700,400italic|PT+Serif:400,400italic", "application")
    != javascript_include_tag("application")
    != csrf_meta_tag()
  body
    #container
      .top
        #logo
          h2 CONGRESO
          h4 Informacion de congresistas
        ul.socialicons
            li!= link_to('Conectar con Facebook', '/auth/facebook', {class: "social-text"})
      != body